{
	"name": "uranio-core",
	"version": "0.0.3",
	"description": "Uranio core",
	"main": "dist/index.js",
	"exports": {
		".": "./dist/server.js",
		"./types": "./dist/server/types.js",
		"./client": "./dist/client.js",
		"./client/types": "./dist/client/types.js"
	},
	"typesVersions": {
		"*": {
			"*": [
				"dist/server.d.ts"
			],
			"types": [
				"dist/server/types.d.ts"
			],
			"client": [
				"dist/client.d.ts"
			],
			"client/types": [
				"dist/client/types.d.ts"
			]
		}
	},
	"repository": "git@github.com:nbl7/uranio-core.git",
	"author": "Andrea Reni <mail@nbl7.net>",
	"scripts": {
		"build": "npx tsc -b",
		"dev": "npx tsc-watch --onSuccess \"node -r source-map-support/register -r dotenv/config ./dist/dev.js\"",
		"generate:compile": "npx esbuild ./src/server/generate.ts --outfile=./generate/_generate_bundled.js --bundle --platform=node",
		"generate:schema": "yarn generate:compile && node ./generate/_generate_bundled.js urn_command=schema"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.2",
		"@types/caller": "^1.0.0",
		"@types/express-fileupload": "^1.2.2",
		"@types/jsonwebtoken": "^8.5.8",
		"esbuild": "^0.14.22",
		"uranio-dev": "git+ssh://git@github.com/nbl7/uranio-dev.git"
	},
	"dependencies": {
		"aws-sdk": "^2.1066.0",
		"bcryptjs": "^2.4.3",
		"caller": "^1.0.1",
		"dateformat": "^3.0.3",
		"dotenv": "^15.0.0",
		"express-fileupload": "^1.2.1",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^5.11.15",
		"uranio-schema": "ssh://git@github.com/nbl7/uranio-schema.git",
		"urn-lib": "git+ssh://git@github.com/nbl7/uranio-lib.git#new"
	}
}

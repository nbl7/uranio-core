{
	"name": "urn-core",
	"version": "0.0.1",
	"description": "Uranio core",
	"main": "dist/src/index.js",
	"types": "dist/src/index.d.ts",
	"scripts": {
		"build": "tsc -b",
		"dev": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register -r module-alias/register ./dist/src/index.js\"",
		"core": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register -r module-alias/register ./dist/.uranio/repo/run.js\""
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@bitbucket.org/nbl7/urn-core.git"
	},
	"keywords": [
		"uranio",
		"urn"
	],
	"author": "Andrea Reni <mail@nbl7.net>",
	"license": "ISC",
	"homepage": "https://bitbucket.org/nbl7/urn-core#readme",
	"dependencies": {
		"urn-core-dep": "bitbucket:nbl7/urn-core-dep"
	},
	"_moduleAliases": {
		"urn_books": "./dist/.uranio/books.js",
		"uranio": "./dist/.uranio/repo/"
	},
	"devDependencies": {
		"urn-core-dep-dev": "bitbucket:nbl7/urn-core-dep-dev"
	}
}

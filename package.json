{
	"name": "urn-core",
	"version": "0.0.1",
	"description": "Uranio core",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc -b",
		"start": "node -r source-map-support/register -r dotenv/config dist/index.js dotenv_config_path=config/dev.env",
		"dev": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register -r dotenv/config ./dist/index.js dotenv_config_path=config/dev.env\""
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@bitbucket.org/nbl7/urn-core.git"
	},
	"keywords": [
		"uranio",
		"urn"
	],
	"author": "Andrea Reni <mail@nbl7.net>",
	"license": "ISC",
	"homepage": "https://bitbucket.org/nbl7/urn-core#readme",
	"devDependencies": {
		"@types/mongoose": "^5.7.36",
		"urn-dev": "git+ssh://git@bitbucket.org/nbl7/urn-dev.git"
	},
	"dependencies": {
		"dotenv": "^8.2.0",
		"mongoose": "^5.10.10",
		"urn-lib": "file:../urn-lib/",
		"urn-mdls": "file:../urn-mdls/"
	}
}
